<UserControl x:Class="StatusBoard.DynamicEquipmentStatusBoard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:StatusBoard"
             mc:Ignorable="d" x:Name="main">
    <DockPanel>
        <Grid>
            <Rectangle Style="{StaticResource ControlBackground}"/>
            <Grid x:Name="statusPanel" Margin="25"/>
            <Rectangle Fill="AliceBlue" Opacity="0"/>
        </Grid>
    </DockPanel>
</UserControl>
