             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ApplicationManagementConsole"
             xmlns:DatabaseControls="clr-namespace:Database.DatabaseControls;assembly=esb2db" x:Class="ApplicationManagementConsole.UserManager"
             mc:Ignorable="d" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="25">
    <Grid>
        <Rectangle Style="{StaticResource ControlBackground}"/>
        <DatabaseControls:DatabaseUsersControl Margin="25" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</UserControl>
